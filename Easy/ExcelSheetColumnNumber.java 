public class Solution {
    public int titleToNumber(String s) {
        char[] sArray = s.toCharArray();
        int sum = 0;
        for(int i=0;i<sArray.length;i++){
           sum += (int)Math.pow(26,sArray.length - i -1) * charToNumber(sArray[i]);
        }
        return sum;
    }
    
    private int charToNumber(char c){
        return (int) c - 64;
    }
}